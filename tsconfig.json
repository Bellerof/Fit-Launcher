{
 "compilerOptions": {
  "strict": true,
  "target": "ESNext",
  "module": "ESNext",
  "moduleResolution": "bundler",
  "allowSyntheticDefaultImports": true,
  "esModuleInterop": true,
  "allowJs": true,
  "jsx": "preserve",
  "jsxImportSource": "solid-js",
  "types": ["vite/client"],
  "noEmit": true,
  "isolatedModules": true,
  "paths": {
   "@/*": ["./src/lib/*"]
  }
 }
}
